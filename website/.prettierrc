{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 120,
	"tabWidth": 2,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"importOrder": ["^@sveltejs/(.*)$", "^\\$lib/(.*)$", "^\\./.*$", "^\\$app/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
